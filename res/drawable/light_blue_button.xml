<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <gradient
          android:startColor="#0da9dc"
          android:endColor="#035a6f"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#035163" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#69a4d1"
          android:endColor="#677b96"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#5F718A" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>